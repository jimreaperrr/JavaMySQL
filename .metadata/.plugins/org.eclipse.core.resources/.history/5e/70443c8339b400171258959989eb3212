package nhan_nguyen_classes;

import java.sql.*;
import java.util.Scanner;


public class Try_Connection extends Var {

	

	public static Connection newConnection () throws SQLException{ 
		
		try {
		
			myConn(); 
			
		}
	
		catch (Exception exc) {
			exc.printStackTrace();
		    }
		
	
	return Var.getMyConn();
	}
}
